{
  "name": "hatsuportal-api",
  "version": "0.0.1",
  "description": "API Service for HatsuPortal Application",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run test && npm run swagger-gen && npm run routes-gen && npm run tsc && npm run copy-swagger-ui",
    "coverage": "npm run test-cov && npm run test-remap",
    "postinstall": "npm run build",
    "lint": "tslint ./*.ts ./lib/*.ts ./src/**/*.ts ./test/**/*.ts",
    "test": "jest --config jest.json --no-cache",
    "tsc": "tsc --project tsconfig.json",
    "tsc:watch": "tsc -w",
    "start": "nodemon",
    "start:prod": "node ./dist/server.js",
    "clean": "rimraf dist",
    "swagger-gen": "tsoa swagger",
    "routes-gen": "tsoa routes",
    "copy-swagger-ui": "mkdirp ./dist/docs && ncp ./src/swagger-ui ./dist/docs/swagger-ui",
    "tsoa": "tsoa"
  },
  "keywords": [
    "api"
  ],
  "author": "HatsuRebals",
  "license": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/api-error-handler": "^1.0.31",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.1.6",
    "@types/node": "^10.5.2",
    "@types/swagger-ui-express": "^3.0.0",
    "@types/knex": "^0.14.16",
    "@types/pg": "^7.4.10",
    "jest": "^23.4.1",
    "mkdirp": "0.5.1",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.2",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "ts-mockito-2": "^2.2.8",
    "ts-node": "^7.0.0",
    "tsconfig-paths": "^3.4.2",
    "tslint": "^5.10.0",
    "typescript": "2.9.2"
  },
  "dependencies": {
    "express": "^4.16.3",
    "swagger-ui-express": "^3.0.10",
    "body-parser": "^1.18.3",
    "tsoa": "^2.1.8",
    "knex": "^0.15.1",
    "@ruanmartinelli/knex-model": "^0.3.0",
    "pg": "^7.4.3",
    "moment": "^2.22.2",
    "lodash": "^4.17.10"
  }
}
